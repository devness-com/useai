{
  "name": "@useai/shared",
  "version": "0.3.0",
  "private": true,
  "description": "Shared types, crypto, constants, and utilities for useai",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.js"
    },
    "./constants/tools": {
      "types": "./dist/constants/tools.d.ts",
      "import": "./dist/constants/tools.js"
    },
    "./crypto": {
      "types": "./dist/crypto/index.d.ts",
      "import": "./dist/crypto/index.js"
    },
    "./validation": {
      "types": "./dist/validation/index.d.ts",
      "import": "./dist/validation/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./daemon": {
      "types": "./dist/daemon/index.d.ts",
      "import": "./dist/daemon/index.js"
    },
    "./frameworks": {
      "types": "./dist/frameworks/index.d.ts",
      "import": "./dist/frameworks/index.js"
    },
    "./scoring": {
      "types": "./dist/scoring/index.d.ts",
      "import": "./dist/scoring/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit -p tsconfig.build.json",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "typescript": "^5.7.3"
  }
}
